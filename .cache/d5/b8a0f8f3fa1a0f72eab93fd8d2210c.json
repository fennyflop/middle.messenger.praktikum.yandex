rl('../../assets/exit-icon.svg') no-repeat center;\r\n}\r\n\r\n.search {\r\n    width: 95%;\r\n    margin-top: 10px;\r\n    background-color: #343434;\r\n    color: #D5D5D5;\r\n    border: none;\r\n    outline: none;\r\n    border-radius: 5px;\r\n    box-sizing: border-box;\r\n    padding: 6px 10px; \r\n}\r\n\r\n.divider {\r\n    height: 1px;\r\n    width: 95%;\r\n    background-color: #343434;\r\n    margin: 10px 0;\r\n}\r\n\r\n.list {\r\n    list-style-type: none;\r\n    margin: 0;\r\n    padding: 0;\r\n    width: 100%;\r\n    height: fit-content;\r\n    display: flex;\r\n    flex-direction: column;\r\n}\r\n\r\n.chat {\r\n    padding: 10px 20px;\r\n    display: flex;\r\n    flex-direction: column;\r\n    justify-content: space-between;\r\n}\r\n\r\n.recipient {\r\n    width: 100%;\r\n    padding: 0 0 10px; \r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: space-between;\r\n    border-bottom: 1px solid #343434;\r\n}\r\n\r\n.recipientAvatar {\r\n    width: 48px;\r\n    height: 48px;\r\n    border-radius: 50%;\r\n}\r\n\r\n.recipientName {\r\n    color: white;\r\n}\r\n\r\n.more {\r\n    width: 24px;\r\n    height: 24px;\r\n    background: url('../../assets/three-dots-icon.svg') center no-repeat;\r\n    background-color: transparent;\r\n    border: none;\r\n    outline: none;\r\n    border-radius: 4px;\r\n    transition: all .2s ease-in-out;\r\n}\r\n\r\n.more:hover {\r\n    cursor: pointer;\r\n    background-color: #343434;\r\n}\r\n\r\n.handlebar {\r\n    display: flex;\r\n    width: 100%;\r\n    align-items: center;\r\n    justify-content: space-between;\r\n    border-top: 1px solid #343434;\r\n    padding: 11px 0 5px;\r\n}\r\n\r\n.attatch {\r\n    width: 24px;\r\n    height: 24px;\r\n    background: url('../../assets/clip.svg') center no-repeat;\r\n    background-color: transparent;\r\n    border: none;\r\n    outline: none;\r\n}\r\n\r\n.send {\r\n    width: 32px;\r\n    height: 32px;\r\n    background: url('../../assets/arrow-left.svg') center no-repeat;\r\n    transform: rotate(180deg);\r\n    background-color: transparent;\r\n    border: none;\r\n    outline: none;\r\n}\r\n.input {\r\n    margin: 0 10px;\r\n    border: none;\r\n    outline: none;\r\n    padding: 10px 20px;\r\n    border-radius: 30px;\r\n    width: 100%;\r\n}"},"lineCount":null}},"error":null,"hash":"8a567a205c51ec00d580892d013ac140","cacheData":{"env":{}}}